{
  "country": {
    "iso": "HU",
    "name": "Hungary"
  },
  "source": "HungaroControl AIS",
  "scraper": {
    "engine": "axios",
    "features": {
      "baseUrl": "https://ais-en.hungarocontrol.hu/aip/2021-06-17/",
      "paths": [
        {
          "route": "/index.html",
          "navigations": [
            {
              "selector": "frame[name=\"eAISNavigationBase\"]",
              "attribute": "src"
            },
            {
              "selector": "frame[name=\"eAISNavigation\"]",
              "attribute": "src"
            }
          ]
        }
      ],
      "chart": {
        "baseUrl": "${baseUrl}/eAIP/",
        "selector": "a[id=\"AD-2.${icao}\"]",
        "attribute": "href"
      }
    }
  }
}
